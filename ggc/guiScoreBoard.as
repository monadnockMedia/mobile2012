package com.mobile.ggc {		import flash.display.MovieClip;			public class guiScoreBoard extends MovieClip {		private var abs:Object = {};		private var _score:Object = {};				public function set score(o:Object):void		{			_score = o;									for each (var s:String in ["Environment","Energy","Economy"]){				abs[s].myScore = _score.abs[s];				totalScore+=o[s]			}														  		}				public function guiScoreBoard() {			// constructor code							abs["Environment"] = this.score1;				rel["Environment"] = this.score1;				abs.Environment.teamName = "Environment";				rel.Environment.teamName = "Environment";								abs["Energy"] = this.score2;				rel["Energy"] = this.score2;				abs.Energy.teamName = "Energy";				rel.Energy.teamName = "Energy";												abs["Economy"] = this.score3;				rel["Economy"] = this.score3;				abs.Economy.teamName = "Economy";				rel.Economy.teamName = "Economy";								}	}	}