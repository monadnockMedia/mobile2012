package com.mobile.ggc{	import flash.events.Event;	public class GameEvent extends Event 	{		// Event types.				public static const EVENT_DEFAULT:String = "default";		public static const ROUND_BEGIN:String = "beginRound"; // Cards dealt to ACTIVE player for decision		public static const ROUND_END:String = "endRound"; // Issue Dealt to ALL Players for group decision		public static const SCORE:String = "score"; // Issue Dealt to ALL Players for group decision		public var data:*;				public function GameEvent(type:String = GameEvent.EVENT_DEFAULT, bubbles:Boolean = false, cancelable:Boolean = false, _data:* = null) 		{			data = _data;			super(type, bubbles, cancelable);		} 		override public function clone():Event {			// Return a new instance of this event with the same parameters.			return new GameEvent(type, bubbles, cancelable);		}	}}