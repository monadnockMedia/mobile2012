package com.mobile.ggc.gui {		import flash.display.MovieClip;			public class guiScore extends MovieClip {										public function guiScore() {			// constructor code		}						public function set teamName(s:String):void		{			this.teamField.text = s;			this.bg.gotoAndStop(s);			myScore = 0;		}				public function set myScore(n:Number):void		{			this.score.text = n.toString();		}	}	}